                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </Headers>
        </config>
        <pd:inputBindings>
            <ResponseActivityInput>
                <asciiContent>
                    <xsl:value-of select="concat($Call-Process/root/param,&quot;Advertiser :&lt;a href=&quot;,$_globalVariables/ns:GlobalVariables/ProjectGV/HrefLine_hostname,&quot;9992/checkadvertiser?advertiser_id=&quot;,$SelectDetailsById/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[2]/value,&quot;> &quot;,$SelectDetailsById/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[2]/value,&quot;-&quot;,$GetAdvertiserName/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[1]/value,&quot;&lt;/a>&quot;,$Related/root/param)"/>
