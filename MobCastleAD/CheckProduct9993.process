    <pd:activity name="Call Process">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>435</pd:x>
        <pd:y>156</pd:y>
        <config>
            <processName>/MobCastleAD/DetailsFormatView_ById.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:for-each select="$SelectDetailsById/jdbcGeneralActivityOutput/unknownResultset[1]">
                    <unknownResultset>
                        <xsl:for-each select="row">
                            <row>
                                <xsl:for-each select="column">
                                    <column>
                                        <name>
                                            <xsl:value-of select="name"/>
                                        </name>
                                        <xsl:if test="not((value/@xsi:nil=(&quot;true&quot;)) or (value/@xsi:nil=(&quot;1&quot;)))">
                                            <value>
                                                <xsl:value-of select="value"/>
                                            </value>
                                        </xsl:if>
                                    </column>
                                </xsl:for-each>
                            </row>
                        </xsl:for-each>
                    </unknownResultset>
                </xsl:for-each>
                <Tablename>
                    <xsl:value-of select="substring-after(substring-before($CheckProduct/ProcessStarterOutput/RequestURI,&quot;?&quot;),&quot;check&quot;)"/>
                </Tablename>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Related">
