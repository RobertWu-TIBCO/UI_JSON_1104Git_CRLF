            <jdbcGeneralActivityInput>
                <statement>
                    <xsl:value-of select="concat(&quot;select offer_id,offer_name,offer_status from offer where product_id=&quot;,$CheckProduct/ProcessStarterOutput/parameters/product_id )"/>
