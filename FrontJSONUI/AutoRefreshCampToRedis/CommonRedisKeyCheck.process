                <xsl:if test="$Start/root/allupdate">
                    <allupdate>
                        <xsl:value-of select="$Start/root/allupdate"/>
                    </allupdate>
                </xsl:if>
            </javaCodeActivityInput>
        </pd:inputBindings>
