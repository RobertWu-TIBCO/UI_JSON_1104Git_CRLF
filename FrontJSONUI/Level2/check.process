            <fullsource>package FrontJSONUI.Level2.check;
import java.util.*;
import java.io.*;
import redis.clients.jedis.Jedis;     
public class checkRedisCounter{
/****** START SET/GET METHOD, DO NOT MODIFY *****/
	protected String[] campaign_id = null;
	protected long clickCount = 0;
	protected long conversionCount = 0;
	public String[] getcampaign_id() {
		return campaign_id;
	}
	public void setcampaign_id(String[] val) {
		campaign_id = val;
	}
	public long getclickCount() {
		return clickCount;
	}
	public void setclickCount(long val) {
		clickCount = val;
	}
	public long getconversionCount() {
		return conversionCount;
	}
	public void setconversionCount(long val) {
		conversionCount = val;
	}
/****** END SET/GET METHOD, DO NOT MODIFY *****/
	public checkRedisCounter() {
	}
	public void invoke() throws Exception {
/* Available Variables: DO NOT MODIFY
	In  : String[] campaign_id
	Out : long clickCount
	Out : long conversionCount
* Available Variables: DO NOT MODIFY *****/
		Jedis jedis = new Jedis("127.0.0.1", 6379,100000);
		jedis.auth("!VMD1kult");
try{
		for (String i : campaign_id) {
			String clickstr=jedis.hget("clickCounter", i);
			String constr=jedis.hget("conversionCounter", i);
			clickCount+=new Integer((clickstr!=null)?clickstr:"0");
			conversionCount+= new Integer((constr!=null)?constr:"0");
		}
}
catch(Exception e){
clickCount+=0;
conversionCount+=0;
}
System.out.println("### total clickCounter : " + clickCount + " \n total conversionCounter : " + conversionCount);}
}
</fullsource>
