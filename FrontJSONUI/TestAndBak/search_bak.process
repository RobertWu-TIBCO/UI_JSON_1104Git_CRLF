    <pd:activity name="SearchSingleTable">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>214</pd:x>
        <pd:y>75</pd:y>
        <config>
            <processName>/FrontJSONUI/Level3/SearchSql.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <sql>
                    <xsl:value-of select="concat(&quot;select &quot;,$SelectColumns/root/columns,&quot; from &quot;,$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot; where &quot;,$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_name like '%&quot;,$Parse-JSON/ns:ActivityOutputClass/root/searchvalue,&quot;%' or &quot;,&#xA;$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_id like '%&quot;,$Parse-JSON/ns:ActivityOutputClass/root/searchvalue,&quot;%'&quot;)"/>
                </sql>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>494</pd:x>
        <pd:y>36</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx:unknownResultset"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <xsl:for-each select="$SearchSingleTable/unknownResultset">
                    <unknownResultset>
                        <xsl:for-each select="row">
                            <row>
                                <xsl:for-each select="column">
                                    <column>
                                        <xsl:if test="name">
                                            <name>
                                                <xsl:value-of select="name"/>
                                            </name>
                                        </xsl:if>
                                        <xsl:if test="value">
                                            <value>
                                                <xsl:value-of select="value"/>
                                            </value>
                                        </xsl:if>
                                    </column>
                                </xsl:for-each>
                            </row>
                        </xsl:for-each>
                    </unknownResultset>
                </xsl:for-each>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SelectColumns">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:description>would not search campaign table</pd:description>
        <pd:x>164</pd:x>
        <pd:y>190</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="columns" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="alltables" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:choose>
                    <xsl:when test="$Parse-JSON/ns:ActivityOutputClass/root/tablename=&quot;advertiser&quot;">
                        <columns>
                            <xsl:value-of select="concat($Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_id,&quot;,&#xA;$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_name&quot;&#xA;)"/>
                        </columns>
                    </xsl:when>
                    <xsl:when test="$Parse-JSON/ns:ActivityOutputClass/root/tablename=&quot;campaign&quot;">
                        <columns>
                            <xsl:value-of select="concat($Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_id,&quot;,&#xA;&quot;offer_id,&quot;,&#xA;&quot;n_id,&quot;,&#xA;$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_status&quot;&#xA;)"/>
                        </columns>
                    </xsl:when>
                    <xsl:otherwise>
                        <columns>
                            <xsl:value-of select="concat($Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_id,&quot;,&#xA;$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_name,&quot;,&#xA;$Parse-JSON/ns:ActivityOutputClass/root/tablename,&quot;_status&quot;&#xA;)"/>
                        </columns>
                    </xsl:otherwise>
                </xsl:choose>
                <alltables>
                    <xsl:value-of select="&quot;advertiser&quot;"/>
                </alltables>
                <alltables>
                    <xsl:value-of select="&quot;product&quot;"/>
                </alltables>
                <alltables>
                    <xsl:value-of select="&quot;offer&quot;"/>
                </alltables>
                <alltables>
                    <xsl:value-of select="&quot;n&quot;"/>
                </alltables>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render-JSON-WithLink">
